static void __surface_gfx_update(struct vmm_surface *sf,
				 int x, int y, int w, int h)
{
	int width = vmm_surface_width(sf);
	int height = vmm_surface_height(sf);

	x = max(x, 0);
	y = max(y, 0);
	x = min(x, width);
	y = min(y, height);
	w = min(w, width - x);
	h = min(h, height - y);

	if (sf->ops && sf->ops->gfx_update) {
		sf->ops->gfx_update(sf, x, y, w, h);
	}
}

